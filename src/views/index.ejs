            <div class="portfolio-info">
              <h4><%=data[i].name_emprendimiento%></h4>
              <p>Cuenta con un anuncio</p>
              <p><%=data[i].Nombre_Anuncio%></p>
              <a href="/ver/<%=data[i].id_emprendimiento%>" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
            </div>
          </div>
  <% } %>
         
          </div>

        </div>
