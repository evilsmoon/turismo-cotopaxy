                <!-- Row -->
                En este apartado usted podra modificar cualquier tipo de Anuncio registrado

                <div class="row">
                    <!-- Column -->
                    <div class="col-lg-8 col-xlg-8 col-md-8">
                        <div class="card">
                            <div class="card-body">
                                <form class="form-horizontal form-material mx-2" action="/UpdateAnuncio" method="POST" >
                                    <div class="form-group">
                                        <label class="col-sm-12">Tus Registros</label>
                                        <div class="col-sm-12">
                                            <input  type="hidden" name="id_anuncio"  value="<%=consulanun.id_anuncios%>" >

                                            <select class="form-select shadow-none form-control-line" name="id_emprendimiento">
                                                <option value="<%=consultaselect.id_emprendimiento%>"><%=consultaselect.name_emprendimiento%></option>
                                                <% for (var i = 0; i < consult.length; i++){ %>
                                                    <option value="<%=consult[i].id_emprendimiento%>">
                                                        <%=consult[i].name_emprendimiento %>
                                                    </option>
                                                <% } %>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-12">Título Anuncio</label>
                                        <div class="col-md-12">
                                            <input type="text"  required name="Nombre_Anuncio"  value="<%=consulanun.Nombre_Anuncio%>"
                                                class="form-control form-control-line">
                                        </div>
                                    </div>
                                    <input  type="hidden" name="id_emprendedor"  value="<%=data.id_emprendedor%>" >

                                    <div class="form-group">
                                        <label class="col-md-12">Detalle</label>
                                        <div class="col-md-12">
                                            <textarea rows="5" name="detalle" class="form-control form-control-line" maxlength="400" required><%=consulanun.detalle%></textarea>
                                        </div>
                                        <span class="focus-input100">Maximo 400 Caracteres</span>
                                    </div>
                                    
                                    
                                    <div class="form-group">
                                        <label class="col-md-12">Fecha inicio del anuncio</label>
                                        <div class="col-md-12">
                                            <input type="datetime-local" value="<%=consulanun.fecha_inicio%>" required name="fecha_inicio" id="fecha_inicio"
                                                class="form-control form-control-line">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-md-12">Fecha final del anuncio</label>
                                        <div class="col-md-12">
                                            <input type="datetime-local" value="<%=consulanun.fecha_fin%>"  required name="fecha_fin" id="fecha_fin"
                                                class="form-control form-control-line">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <button class="btn btn-success text-white" onclick="check()">Modificar Anuncio</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <script>

                        function check(){

                        
                                if(document.getElementById("fecha_inicio").value>document.getElementById("fecha_fin").value){
                alert("Llene el campo Fecha final con una fecha superior a la del Fecha inicio ");
                document.getElementById("fecha_fin").value = "";
                    }
                            }
                            
                   
                    </script>
